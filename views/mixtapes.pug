extends layout

block body
  section.mixtapes
    .container
      .section-title
        h2 Mixtapes
        a.underline-hover(href="/mixtapes") View All           
      .mixtapes__feed
        if !mixtapes || mixtapes.length === 0
          h4 No Mixtapes Found
        else 
          for mixtape in mixtapes
            +mixtapeListing(mixtape)